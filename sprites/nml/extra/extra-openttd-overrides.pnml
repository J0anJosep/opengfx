
/* In early OpenTTD versions, three overrides had to be defined
 * within the base set for legacy reasons
 */
if (openttd_version < version_openttd(1, 2, 0, 23232)) {
	engine_override("\44\44\22\02", "\44\44\01\11"); // UKRS addons modifies UKRS
	engine_override("\6D\62\04\02", "\6D\62\04\01"); // DBSetXL ECS extension modifies DBSetXL
	engine_override("Meo ", "Meo\17");               // LV4cut modifies LV4
}
